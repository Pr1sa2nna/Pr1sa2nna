n=int(input ('enter n')) 
l=[] 
t=[] 
for I in range (n): 
     x=int(input ()) 
     l.append(x) 
x=0 
for I in range (n):
    if l[I]>=0: 
       t.append(l[I]) 
       x=x+1  
for I in range(x,n) 
    t.append(0) 
print(t) 
c=0 
for I in l: 
    if l[I]>0: 
       c=c+1 
print(c) 



