l=input().split() 
print(l) 
t=[] 
for I in l: 
     t.append(int(I)) 
print(t) 
x=[] 
for I in t: 
      if I not in x: 
          x.append(I) 
print(x) 
x.sort(reverse= True) 
print(x) 
print(x[2]) 
  

